{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "make-dev",
      "type": "shell",
      "command": "make",
      "args": ["dev"],
      "isBackground": true,
      "problemMatcher": {
        "pattern": {
          "regexp": "^.*$",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^.*starting.*",
          "endsPattern": "^.*listening.*|^.*ready.*"
        }
      }
    }
  ]
}